extends layout
		
block content
	div(class="container")
		div(class="row")
			div(class="col-md-8")
				h1= title
					div(class="row move-down justify-content-center")
					div(class="col-auto")
						table(class="table table-striped table-hover table-responsive")
												thead
														tr
																th DataIrLaikas
																th IsilauzimuKiekis
												tbody									
													each item in data
														tr
															td=item.Data
															td
															td=item.Kiekis
